{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "ES2021",
            "ES2021.String"
        ],
        "resolveJsonModule": true,
        "strict": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noFallthroughCasesInSwitch": true,
    
        "noImplicitAny": false,
    },
    "include": [
        "*.ts"
    ],
    "files": [
        "index.ts",
    ]
}